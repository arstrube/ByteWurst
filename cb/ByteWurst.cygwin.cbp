<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ByteWurst(Cygwin)" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option platforms="Windows;" />
				<Option output="..\out\$(PROJECT_NAME)$(TARGET_NAME)" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\out\" />
				<Option object_output="..\out\" />
				<Option type="1" />
				<Option compiler="cygwin" />
			</Target>
			<Target title="Test">
				<Option platforms="Windows;" />
				<Option output="..\out\$(PROJECT_NAME)$(TARGET_NAME)" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\out\" />
				<Option object_output="..\out\" />
				<Option type="1" />
				<Option compiler="cygwin" />
				<ExtraCommands>
					<Add after="$(TARGET_OUTPUT_FILE)" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-Werror" />
			<Add option="-include MemoryLeakDetectorNewMacros.h" />
			<Add directory=".." />
			<Add directory="..\CppUTest\include\CppUTest" />
			<Add directory="..\CppUTest\include\CppUTestExt" />
			<Add directory="..\fakes" />
			<Add directory="..\StorageAdapter" />
			<Add directory="..\Settings" />
			<Add directory="..\SettingsFactory" />
			<Add directory="..\..\system" />
		</Compiler>
		<Linker>
			<Add library="..\..\unittest\CppUTest\lib\libCppUTestExt.a" />
			<Add library="..\..\unittest\CppUTest\lib\libCppUTest.a" />
		</Linker>
		<Unit filename="..\ByteWurst\ByteWurst.cpp" />
		<Unit filename="..\ByteWurst\ByteWurst.h" />
		<Unit filename="..\ByteWurst\ByteWurst_Tests.cpp" />
		<Unit filename="..\UnitTest.cpp" />
		<Unit filename="..\fakes\FormatOutputSpy.h" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
